package com.yoonshop.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MainController {

	@GetMapping("/main")
	public String home() {
		System.out.println("메인");
		return "main";
	}
	
	@GetMapping("")
	public String hom() {
		return "/main";
	}
	
	
}
